	<select id="findAllList" resultType="VCcmTeam">
		SELECT
		<include refid="vCcmTeamColumns" />
		FROM v_ccm_team a
		<include refid="vCcmTeamJoins" />
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
		</where>
	</select>

	<select id="findByParentIdsLike" resultType="VCcmTeam">
		SELECT
		a.id,
		a.parent_id AS "parent.id",
		a.parent_ids
		FROM v_ccm_team a
		<include refid="vCcmTeamJoins" />
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			AND a.parent_ids LIKE
			#{parentIds}
		</where>
	</select>
	
	<select id="findByOfficeParentIdsLike" resultType="VCcmTeam" parameterType="Office">
		SELECT
		<include refid="vCcmTeamColumnsRest" />
		FROM v_ccm_team a

		LEFT JOIN sys_office b ON a.office_id = b.id
		WHERE
			a.del_flag = '0'
			AND b.id = #{id}
			OR b.parent_ids LIKE 
			<if test="dbName == 'oracle'">'%'||#{id}||'%'</if>
            <if test="dbName == 'mssql'">'%'+#{id}+'%'</if>
            <if test="dbName == 'mysql'">concat('%',#{id},'%')</if>
	</select>
	
	<select id="findMyFriendAvailablePage" resultType="VCcmTeam">
		SELECT
		<include refid="vCcmTeamColumnsRest" />
		FROM v_ccm_team a
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			<if
				test="companyId != null and companyId.id != null and companyId.id != ''">
				AND a.company_id = #{companyId.id}
			</if>
			
			<if test="name != null and name != ''">
				AND a.name LIKE
				<if test="dbName == 'oracle'">'%'||#{name}||'%'</if>
				<if test="dbName == 'mssql'">'%'+#{name}+'%'</if>
				<if test="dbName == 'mysql'">concat('%',#{name},'%')</if>
			</if>
			<if test="groupId != null and groupId != ''">
				AND a.id NOT IN
				(SELECT user_id FROM ccm_user_relationship WHERE group_id = #{groupId}) 
			</if> 
			
		</where>
	</select>
	
	<select id="findUserByOffice" resultType="VCcmTeam">
		SELECT
			a.id AS "id",
			a.`name` AS "name",
			a.photo AS "photo",
			o.`name` AS "more1",
			a.`status` AS "status"
		FROM v_ccm_team a, sys_office o
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			AND a.office_id = o.id
			<if test="more1 != null and more1 != ''">
				AND o. parent_ids LIKE
				<if test="dbName == 'oracle'">'%'||#{more1}||'%'</if>
				<if test="dbName == 'mssql'">'%'+#{more1}+'%'</if>
				<if test="dbName == 'mysql'">concat('%',#{more1},'%')</if>
			</if>
		</where>
	</select>
	
	
	<select id="findByTypes" resultType="com.arjjs.ccm.tool.SearchTab">
		SELECT
			a.id AS "value1",
			a.name AS "value2"
		FROM sys_office a
		<where>
			a.del_flag = 0
			AND a.type = 2
		</where>
		ORDER BY a.sort ASC
	</select>
	
	
	<insert id="insert">
		INSERT INTO v_ccm_team(
		id,
		company_id,
		office_id,
		login_name,
		password,
		no,
		name,
		email,
		phone,
		mobile,
		user_type,
		photo,
		login_ip,
		login_date,
		login_flag,
		create_by,
		create_date,
		update_by,
		update_date,
		remarks,
		del_flag,
		user_id,
		sex,
		nation,
		politics,
		iden_num,
		birthday,
		grade,
		service,
		prof_expertise,
		education,
		fix_tel,
		team_type,
		status,
		more1,
		more2
		) VALUES (
		#{id},
		#{companyId.id},
		#{office.id},
		#{loginName},
		#{password},
		#{no},
		#{name},
		#{email},
		#{phone},
		#{mobile},
		#{userType},
		#{photo},
		#{loginIp},
		#{loginDate},
		#{loginFlag},
		#{createBy.id},
		#{createDate},
		#{updateBy.id},
		#{updateDate},
		#{remarks},
		#{delFlag},
		#{user.id},
		#{sex},
		#{nation},
		#{politics},
		#{idenNum},
		#{birthday},
		#{grade},
		#{service},
		#{profExpertise},
		#{education},
		#{fixTel},
		#{teamType},
		#{status},
		#{more1},
		#{more2}
		)
	</insert>

	<update id="update">
		UPDATE v_ccm_team SET
		company_id = #{companyId.id},
		office_id = #{office.id},
		login_name = #{loginName},
		password =
		#{password},
		no = #{no},
		name = #{name},
		email = #{email},
		phone =
		#{phone},
		mobile = #{mobile},
		user_type = #{userType},
		photo = #{photo},
		login_ip = #{loginIp},
		login_date = #{loginDate},
		login_flag =
		#{loginFlag},
		update_by = #{updateBy.id},
		update_date = #{updateDate},
		remarks = #{remarks},
		user_id = #{user.id},
		sex = #{sex},
		nation =
		#{nation},
		politics = #{politics},
		iden_num = #{idenNum},
		birthday =
		#{birthday},
		grade = #{grade},
		service = #{service},
		prof_expertise =
		#{profExpertise},
		education = #{education},
		fix_tel = #{fixTel},
		team_type = #{teamType},
		status = #{status},
		more1 = #{more1},
		more2 = #{more2}
		WHERE id =
		#{id}
	</update>

	<update id="updateParentIds">
		UPDATE v_ccm_team SET
		parent_id = #{parent.id},
		parent_ids = #{parentIds}
		WHERE id = #{id}
	</update>

	<update id="delete">
		UPDATE v_ccm_team SET
		del_flag = #{DEL_FLAG_DELETE}
		WHERE id = #{id} OR parent_ids LIKE
		'%,'||#{id}||',%'
	</update>


	<select id="GetUserByArea" resultType="VCcmTeam">
		SELECT
		<include refid="vCcmTeamColumns" />
		FROM
		v_ccm_team a
		<include refid="vCcmTeamJoins" />
		LEFT JOIN sys_office b ON a.office_id = b.id
		LEFT JOIN
		sys_area a2 ON a2.id = b.area_id
		WHERE
		a2.id = #{id}
		And a.del_flag = '0'
		And c.del_flag = '0'
		And o3.del_flag = '0'
		And b.del_flag = '0'
		And a2.del_flag = '0'
	</select>
	
	
	
	
</mapper>