    <select id="findSolveByArea" resultType="com.arjjs.ccm.tool.EchartType">
		SELECT
			(
				CASE c.param_str
				WHEN '1' THEN community.name
				WHEN '2' THEN street.name
				ELSE street.name
				END
			) AS "type",
			FORMAT(
				SUM(CASE WHEN a.status = '02' THEN '1' ELSE '0' END) * 100 / count(a.id),
				2
			) AS "value"
		FROM
			ccm_event_incident a
		LEFT JOIN sys_area net ON a.case_place = net.id
		LEFT JOIN sys_area community ON community.id = net.parent_id
		LEFT JOIN sys_area street ON street.id = community.parent_id
		LEFT JOIN sys_config c ON c.id = 'system_level'
		WHERE
			a.del_flag = 0
			AND net.del_flag = 0
			AND community.del_flag = 0
			AND street.del_flag = 0
		AND (
			a.happen_date &gt;= date_format(
				DATE_SUB(curdate(), INTERVAL 0 MONTH),
				'%Y-%m'
			)
			AND a.happen_date &lt; date_format(
				DATE_SUB(curdate(), INTERVAL - 1 MONTH),
				'%Y-%m'
			)
		)
		GROUP BY
			(
				CASE c.param_str
				WHEN '1' THEN community.name
				WHEN '2' THEN street.name
				ELSE street.name
				END
			)
		ORDER BY
			--FORMAT(
				SUM(CASE WHEN a.status = '02' THEN '1' ELSE '0' END) * 100 / count(a.id),
				2
			) DESC
		LIMIT 10
	</select>

    <select id="findSolveByMon" resultType="com.arjjs.ccm.tool.EchartType">
        SELECT
        date_format(a.happen_date,'%Y-%m') as "type",
        FORMAT(
        sum(a.status='02') * 100 / count(*),
        2
        ) AS "value"
        FROM
        ccm_event_incident a
        <include refid="ccmEventIncidentJoins"/>
        <where>
            a.del_flag = 0
            and
            (a.happen_date &gt; date_format(
            DATE_SUB(curdate()
            , INTERVAL 12
            MONTH), '%Y-%m')
            AND
            a.happen_date &lt;
            date_format( DATE_SUB(curdate(),
            INTERVAL 0
            MONTH), '%Y-%m'))
        </where>
        GROUP BY
        concat(
        YEAR (a.happen_date),
        MONTH (a.happen_date)
        )
        ORDER BY
        concat(
        YEAR (a.happen_date),
        IF (
        MONTH (a.happen_date) &gt; 9,
        MONTH (a.happen_date),
        concat("0", MONTH(a.happen_date))
        )) ASC
        LIMIT 12
    </select>

    <select id="findSumByMon" resultType="com.arjjs.ccm.tool.EchartType">
        SELECT
        concat(
        YEAR
        (a.happen_date),
        "-",
        MONTH (a.happen_date)
        ) AS "type",
        count(*) AS "value"
        FROM
        ccm_event_incident a
        <where>
            a.del_flag = '0'
            <if test="eventType != null and eventType != ''">
                AND a.event_type = #{eventType}
            </if>
            <if test="eventTypes != null and eventTypes.length > 0">
                AND (
                <foreach collection="eventTypes" item="m"  separator=" OR " >
                    <if test="m != null and m != ''">
                        a.event_type = #{m}
                    </if>
                </foreach>
                )
            </if>
            <if test="status != null and status != ''">
                AND a.status = #{status}
            </if>
            and
            (a.happen_date &gt; date_format(
            DATE_SUB(curdate()
            , INTERVAL 11
            MONTH), '%Y-%m')
            AND
            a.happen_date &lt;
            date_format( DATE_SUB(curdate(),
            INTERVAL -1
            MONTH), '%Y-%m'))
        </where>
        GROUP BY
        concat(
        YEAR (a.happen_date),
        MONTH
        (a.happen_date)
        )
        ORDER BY
        concat(
        YEAR (a.happen_date),
        IF (
        MONTH (a.happen_date) &gt; 9,
        MONTH (a.happen_date),
        concat("0", MONTH(a.happen_date))
        )) ASC
        LIMIT 12
    </select>

