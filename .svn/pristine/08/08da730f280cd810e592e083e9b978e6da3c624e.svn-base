		a.num_flee AS "numFlee",
		a.num_arrest AS "numArrest",
		a.case_over_day AS "caseOverDay",
		a.case_solve AS "caseSolve",
		a.file1 AS "file1",
		a.file2 AS "file2",
		a.file3 AS "file3",
