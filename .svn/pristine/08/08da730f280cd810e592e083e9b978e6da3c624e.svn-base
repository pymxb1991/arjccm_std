        t1.report_type


    </select>
    <select id="queryAll" resultType="com.arjjs.ccm.modules.dma.eventheme.entity.EventIncidentVo">
        SELECT
        t1.id,
        t1.case_place,
        (CASE
        WHEN t1.report_type = '1' THEN "APP录入"
        WHEN t1.report_type = '2' THEN "短信录入"
        WHEN t1.report_type = '3' THEN "热线录入"
        WHEN t1.report_type = '4' THEN "网站录入"
        WHEN t1.report_type = '5' THEN "机顶盒录入"
        ELSE '手动录入' END) report_type
        FROM
        ccm_event_incident AS t1
        WHERE
        t1.del_flag = '0'
        <if test="beginHappenDate != null and beginHappenDate != '' ">
            and t1.happen_date &gt;= #{beginHappenDate}
        </if>
        <if test="endHappenDate != null and endHappenDate != '' ">
            and t1.happen_date &lt; #{endHappenDate}
        </if>
    </select>

    <resultMap id="ForceResultMap" type="com.arjjs.ccm.tool.EchartForce">
        <collection property="nodes" ofType="com.arjjs.ccm.tool.EchartForceNodes">
            <result property="category" column="nodes.category" javaType="java.lang.Integer"/>
            <result property="name" column="nodes.name" javaType="java.lang.String"/>
            <result property="value" column="nodes.value" javaType="java.lang.Integer"/>
            <result property="label" column="nodes.label" javaType="java.lang.String"/>
        </collection>
        <collection property="links" ofType="com.arjjs.ccm.tool.EchartForceLinks">
            <result property="source" column="links.source" javaType="java.lang.String"/>
            <result property="target" column="links.target" javaType="java.lang.String"/>
        </collection>
    </resultMap>

    <select id="getEventForForce" resultMap="ForceResultMap">
        SELECT
        1 AS "nodes.category",
        a.id AS "nodes.name",
        a.case_name AS "nodes.label",
        15 AS "nodes.value",
        a.id AS "links.source",
        a.cul_paperid AS "links.target"
        FROM ccm_event_incident a
        <where>
            a.del_flag = #{DEL_FLAG_NORMAL}
            <if test="culPaperid != null and culPaperid != ''">
                AND a.cul_paperid = #{culPaperid}
            </if>
        </where>
    </select>

    <select id="getcountevent" resultType="com.arjjs.ccm.tool.EchartType">
		SELECT
		IFNULL( count(1), 0 ) AS value ,
		DATE_FORMAT( happen_date, '%Y-%m' ) AS type
		FROM
		ccm_event_incident
		WHERE
		(
			happen_date &gt; date_format(
			DATE_SUB(curdate(), INTERVAL #{num} MONTH),
			'%Y-%m'
			)
			AND happen_date &lt; date_format(
			DATE_SUB(curdate(), INTERVAL 0 MONTH),
			'%Y-%m'
			)
		)
		GROUP BY
		DATE_FORMAT( happen_date, '%Y-%m' )
	</select>

    <sql id="officeColumns">
		a.id,
		a.parent_id AS "parent.id",
		a.parent_ids,
		a.area_id AS "area.id",
		a.code,
		a.name,
		a.sort,
		a.type,
		a.grade,
		a.address,
		a.zip_code,
		a.master,
		a.phone,
		a.fax,
		a.email,
		a.remarks,
		a.create_by AS "createBy.id",
		a.create_date,
		a.update_by AS "updateBy.id",
		a.update_date,
		a.del_flag,
		a.useable AS useable,
		a.primary_person AS "primaryPerson.id",
		a.deputy_person AS "deputyPerson.id",
		p.name AS "parent.name",
		ar.name AS "area.name",
		ar.parent_ids AS "area.parentIds",
		pp.name AS "primaryPerson.name",
		dp.name AS "deputyPerson.name"
	</sql>

    <sql id="officeJoins">
		LEFT JOIN sys_office p ON p.id = a.parent_id
		LEFT JOIN sys_area ar ON ar.id = a.area_id
		LEFT JOIN sys_user pp ON pp.id = a.primary_person
		LEFT JOIN sys_user dp ON dp.id = a.deputy_person
    </sql>

    <select id="findOfficeList" resultType="Office">
        SELECT
        <include refid="officeColumns"/>
        FROM sys_office a
        <include refid="officeJoins"/>
        WHERE a.del_flag = #{DEL_FLAG_NORMAL}
        <!-- 数据范围过滤,只能看到自己区域及子区域的和自己上一级区域的网格员 -->
        AND (a.id = #{currentUser.office.parentId}
        OR a.id = #{currentUser.office.id}
        OR a.parent_ids LIKE concat('%',#{currentUser.office.id},'%'))
        ORDER BY a.code
    </select>

    <!-- 事件规模统计 -->
    <select id="findEventGuiMoData" resultType="com.arjjs.ccm.tool.EchartType">
		SELECT
			a.label AS 'type',
			ifnull( bb.one, 0 ) AS 'value'
		FROM
			sys_dict a
			LEFT JOIN ( SELECT b.case_scope, count( * ) AS one FROM ccm_event_incident b WHERE b.del_flag = 0 GROUP BY b.case_scope ) AS bb ON a.`value` = bb.case_scope
		WHERE
			a.type = "ccm_event_scale"
		ORDER BY
			ifnull( bb.one, 0 ) DESC
	</select>
    <!-- 事件规模统计总数 -->
    <select id="findAllEventNum" resultType="com.arjjs.ccm.tool.EchartType">
		SELECT count(*) AS 'value' FROM ccm_event_incident b WHERE b.del_flag = 0
	</select>

    <select id="selectByAreaIdAndName" resultType="com.arjjs.ccm.modules.ccm.pop.entity.CcmAreaPoint" parameterType="com.arjjs.ccm.modules.ccm.sys.entity.CcmAreaPointVo">
        SELECT
        t1.id,
        t1.area_point,
        t1.event_scale AS "type"
        FROM
        ccm_event_incident t1
        WHERE
        t1.del_flag = 0 and t1.area_point != ""
        <if test="areaId != null and areaId !=''">
            and t1.case_place = #{areaId}
        </if>
        <if test="name != null and name !=''">
            and t1.case_name like  '%${name}%'
        </if>
    </select>

    <select id="findDispatch" resultType="CcmEventIncident">
        SELECT DISTINCT
        <include refid="ccmEventIncidentColumnsWithUser"/>
        ,a21.id AS "bphAlarmInfoId"
        FROM ccm_event_incident a
        <include refid="ccmEventIncidentJoinsUser"/>
        <include refid="ccmEventIncidentJoins"/>
        <include refid="ccmBphAlarmInfoJoins"/>
<!--        LEFT JOIN ccm_event_casedeal c ON a.id = c.obj_id-->
        <where>
            a.del_flag = #{DEL_FLAG_NORMAL}
            <if test="userArea != null and userArea.parentIds != null and userArea.parentIds != ''">
                AND (a20.parent_ids LIKE CONCAT('%',#{userArea.id},'%') or a.case_place = #{userArea.id})
            </if>
            <if test="caseName != null and caseName != ''">
                AND a.case_name LIKE
                <if test="dbName == 'oracle'">'%'||#{caseName}||'%'</if>
                <if test="dbName == 'mssql'">'%'+#{caseName}+'%'</if>
                <if test="dbName == 'mysql'">concat('%',#{caseName},'%')</if>
            </if>
            <if test="beginHappenDate != null and beginHappenDate != ''">
                AND a.happen_date &gt;= #{beginHappenDate}
            </if>
            <if test="endHappenDate != null and endHappenDate != ''">
                AND a.happen_date &lt;= #{endHappenDate}
            </if>
            <if test="area != null and area.id != null and area.id != ''">
                AND a.case_place = #{area.id}
            </if>
            <if test="eventScale != null and eventScale != ''">
                AND a.event_scale = #{eventScale}
            </if>
            <if test="eventType != null and eventType != ''">
                AND a.event_type = #{eventType}
            </if>
            <if test="eventKind != null and eventKind != ''">
                AND a.event_kind = #{eventKind}
            </if>
            <if test="status != null and status != ''">
                AND a.status = #{status}
            </if>
            <if test="property != null and property != ''">
                AND a.property = #{property}
            </if>
            <if test="historyLegacy != null and historyLegacy != ''">
                AND a.history_legacy = #{historyLegacy}
            </if>
            AND (a.doing = "0" OR a.doing IS NULL)
<!--            AND (c.id IS NULL OR c.handle_status = '04')-->
        </where>
        <choose>
            <when test="page !=null and page.orderBy != null and page.orderBy != ''">
                ORDER BY ${page.orderBy}
            </when>
            <otherwise>
                ORDER BY a.stick DESC ,a.update_date DESC
            </otherwise>
        </choose>
    </select>

    <select id="findArchive" resultType="CcmEventIncident">
        SELECT DISTINCT
        <include refid="ccmEventIncidentColumnsWithUser"/>
        ,a21.id AS "bphAlarmInfoId"
        FROM ccm_event_incident a
        <include refid="ccmEventIncidentJoinsUser"/>
        <include refid="ccmEventIncidentJoins"/>
        <include refid="ccmBphAlarmInfoJoins"/>
        <where>
            a.del_flag = #{DEL_FLAG_NORMAL}
            <if test="userArea != null and userArea.parentIds != null and userArea.parentIds != ''">
                AND (a20.parent_ids LIKE CONCAT('%',#{userArea.id},'%') or a.case_place = #{userArea.id})
            </if>
            <if test="caseName != null and caseName != ''">
                AND a.case_name LIKE
                <if test="dbName == 'oracle'">'%'||#{caseName}||'%'</if>
                <if test="dbName == 'mssql'">'%'+#{caseName}+'%'</if>
                <if test="dbName == 'mysql'">concat('%',#{caseName},'%')</if>
            </if>
            <if test="beginHappenDate != null and beginHappenDate != ''">
                AND a.happen_date &gt;= #{beginHappenDate}
            </if>
            <if test="endHappenDate != null and endHappenDate != ''">
                AND a.happen_date &lt;= #{endHappenDate}
            </if>
            <if test="area != null and area.id != null and area.id != ''">
                AND a.case_place = #{area.id}
            </if>
            <if test="eventScale != null and eventScale != ''">
                AND a.event_scale = #{eventScale}
            </if>
            <if test="eventType != null and eventType != ''">
                AND a.event_type = #{eventType}
            </if>
            <if test="eventKind != null and eventKind != ''">
                AND a.event_kind = #{eventKind}
            </if>
            <if test="property != null and property != ''">
                AND a.property = #{property}
            </if>
            <if test="historyLegacy != null and historyLegacy != ''">
                AND a.history_legacy = #{historyLegacy}
            </if>
            AND a.status = "02"
        </where>
        <choose>
            <when test="page !=null and page.orderBy != null and page.orderBy != ''">
                ORDER BY ${page.orderBy}
            </when>
            <otherwise>
                ORDER BY a.stick DESC ,a.update_date DESC
            </otherwise>
        </choose>
    </select>
</mapper>