    <select id="getItemByProperty" resultType="com.arjjs.ccm.tool.EchartType">
        SELECT
        b.label AS "type",
        COUNT(1) AS "value"
        FROM
        ccm_event_incident a
        <include refid="ccmEventIncidentJoins"/>
        LEFT JOIN sys_dict b
        ON
        b.`value` = a.property
        AND b.type="bph_alarm_info_typecode"
        <where>
            a.del_flag=0
        </where>
