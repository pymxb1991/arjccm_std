			when 01 then '未处理' 
			when 02 then '处理中' 
			when 03 then '已完成'
			else '' end) "statusLable"
	</sql>

    <sql id="ccmEventIncidentJoins">
		LEFT JOIN sys_area a20 ON a20.id = a.case_place
	</sql>
    <sql id="ccmBphAlarmInfoJoins">
        LEFT JOIN bph_alarm_info a21 ON a21.id = a.id
    </sql>
    <sql id="ccmEventIncidentJoinsCaseGrad">
		LEFT JOIN sys_dict b21 ON b21.`value` = a.event_scale AND
		b21.type="ccm_case_grad"
	</sql>

    <sql id="ccmEventIncidentJoinsUser">
		LEFT JOIN sys_user b ON b.id = a.create_by
	</sql>

    <select id="get" resultType="CcmEventIncident">
        SELECT
        <include refid="ccmEventIncidentColumns"/>
        FROM ccm_event_incident a
        <include refid="ccmEventIncidentJoins"/>
        WHERE a.id = #{id}
    </select>

    <select id="getIncidents" resultType="CcmEventIncident">
        SELECT
        <include refid="ccmEventIncidentColumnsWithUser"/>
        ,a21.id AS "bphAlarmInfoId"
        FROM ccm_event_incident a
        <include refid="ccmEventIncidentJoinsUser"/>
        <include refid="ccmEventIncidentJoins"/>
        <include refid="ccmBphAlarmInfoJoins"/>
        LEFT JOIN ccm_event_place place ON place.event_id = a.id
        WHERE a.del_flag = #{DEL_FLAG_NORMAL} AND place.place_id = #{placeId}
    </select>

    <select id="getIncidentPlace" resultType="string">
        SELECT
        	a.event_id AS "id"
        FROM ccm_event_place a
        WHERE a.place_id = #{placeId}
    </select>

    <delete id="deleteIncidentPlace">
		DELETE FROM ccm_event_place
		WHERE place_id = #{placeId}
	</delete>

	<insert id="saveIncidentPlace">
		INSERT INTO ccm_event_place(
			id,
			place_id,
			event_id
		) VALUES (
			#{id},
			#{placeId},
			#{eventId}
		)
	</insert>

    <!-- 首页师生安全统计数 -->
    <select id="findCountSchoolPeople" resultType="CcmEventIncident">
		SELECT
			count( a.id )  AS "count"
		FROM
			ccm_event_incident a
		WHERE
			a.del_flag = 0
			AND a.event_kind = "02"
	</select>
    <!-- 查询相关案事件 -->
    <select id="findOtherId" resultType="CcmEventIncident">
        SELECT
        <include refid="ccmEventIncidentColumns"/>
        FROM ccm_event_incident a
        <include refid="ccmEventIncidentJoins"/>
        <where>
            a.del_flag = #{DEL_FLAG_NORMAL}
            <if test="otherId != null and otherId != ''">
                AND a.other_id = #{otherId}
            </if>
        </where>
        <choose>
            <when test="page !=null and page.orderBy != null and page.orderBy != ''">
                ORDER BY ${page.orderBy}
            </when>
            <otherwise>
                ORDER BY a.happen_date DESC
            </otherwise>
        </choose>
    </select>

    <select id="getItemByPropertyec" resultType="com.arjjs.ccm.tool.EchartType">
        SELECT
        IF
        ( a.property IS NULL || a.property = '', '99', a.property ) AS typeO,
        IF
        ( b.label IS NULL || b.label = '', '未知', b.label ) AS "type",
        COUNT(*) AS "value"
        FROM
        ccm_event_incident a
        LEFT JOIN sys_dict b
        ON
        b.`value` = a.property
        AND b.type="ccm_case_natu"
        <where>
            a.del_flag=0 AND a.event_kind="02"
        </where>
        GROUP BY
        a.property
    </select>

    <!-- 师生相关案事件 -->
    <select id="findPageStudent" resultType="CcmEventIncident">
        SELECT
        <include refid="ccmEventIncidentColumnsWithUser"/>
        FROM ccm_event_incident a
        <include refid="ccmEventIncidentJoinsUser"/>
        <include refid="ccmEventIncidentJoins"/>
        <where>
            a.del_flag = #{DEL_FLAG_NORMAL}
            AND a.event_kind = "02"
            <if test="caseName != null and caseName != ''">
                AND a.case_name LIKE
                <if test="dbName == 'oracle'">'%'||#{caseName}||'%'</if>
                <if test="dbName == 'mssql'">'%'+#{caseName}+'%'</if>
                <if test="dbName == 'mysql'">concat('%',#{caseName},'%')</if>
            </if>
            <if
                    test="beginHappenDate != null and endHappenDate != null and beginHappenDate != '' and endHappenDate != ''">
                AND a.happen_date BETWEEN #{beginHappenDate} AND
                #{endHappenDate}
            </if>
            <if test="area != null and area.id != null and area.id != ''">
                AND a.case_place = #{area.id}
            </if>
            <if test="eventScale != null and eventScale != ''">
                AND a.event_scale = #{eventScale}
            </if>
            <if test="eventType != null and eventType != ''">
                AND a.event_type = #{eventType}
            </if>
            <if test="status != null and status != ''">
                AND a.status = #{status}
            </if>
            <if test="property != null and property != ''">
                AND a.property = #{property}
            </if>
        </where>
        <choose>
            <when test="page !=null and page.orderBy != null and page.orderBy != ''">
                ORDER BY ${page.orderBy}
            </when>
            <otherwise>
                ORDER BY a.happen_date DESC
            </otherwise>
        </choose>
    </select>


