.unfold {
	width: 50px;
    height: 50px;
    position: absolute;
    left: 3px;
    bottom: 3px;
    background-color: #0e54a9;
    z-index: 999;
    cursor: pointer;
}

.relevance-bg {
	width: 50px;
	height: 50px;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 9;
}

.br-yuan {
	width: 40px;
	height: 40px;
	border: 2px solid #fff;
	border-radius: 50%;
	background: url(../images/Shape.png) no-repeat 50% 50%;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-top: -20px;
    margin-left: -20px;
}

.relevance {
	background: #fff;
	position: absolute;
	left: 0;
	bottom: 0%;
	overflow: hidden;
}

.re-close {
	width: 30px;
	height: 30px;
	border-radius: 50%;
	background: url(../images/Shape2.png) no-repeat center;
	position: absolute;
	right: 0;
	top: 0;
}

.re-header {
	width: 36px;
	height: 36px;
	position: absolute;
	right: 0;
	top: 0;
	background: #0e54a9
}

