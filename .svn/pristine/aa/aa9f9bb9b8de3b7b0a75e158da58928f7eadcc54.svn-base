package com.arjjs.ccm.modules.ccm.org.entity;

import java.util.List;

/**
 * 机构设置绑定Entity
 * @author maoxb
 * @version 2019-08-29
 */
public class CcmOrgDeviceInfo {

	private static final long serialVersionUID = 1L;
	private String orgId;		// 机构ID
	private List<CcmDeviceVo> deviceList;	//设备列表

	public String getOrgId() {
		return orgId;
	}

	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}

	public List<CcmDeviceVo> getDeviceList() {
		return deviceList;
	}

	public void setDeviceList(List<CcmDeviceVo> deviceList) {
		this.deviceList = deviceList;
	}
}