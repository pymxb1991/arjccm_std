<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arjjs.ccm.modules.ccm.log.dao.CcmLogTailDao">

	<sql id="ccmLogTailColumns">
		a.id AS "id",
		a.relevance_id AS "relevanceId",
		a.relevance_table AS "relevanceTable",
		a.type AS "type",
		a.tail_case AS
		"tailCase",
		a.tail_content AS "tailContent",
		a.tail_person AS
		"tailPerson",
		IFNULL( a.tail_time ,now()) AS "tailTime",
		a.more1 AS "more1",
		a.more2 AS
		"more2",
		a.more3 AS "more3",
		a.create_by AS "createBy.id",
		a.create_date
		AS "createDate",
		a.update_by AS "updateBy.id",
		a.update_date AS
		"updateDate",
		a.remarks AS "remarks",
		a.del_flag AS "delFlag",
		a.file AS "file"
		,sysU.`name` as "createBy.name"
	</sql>

	<sql id="ccmLogTailJoins">
		LEFT JOIN sys_user sysU on a.create_by = sysU.id
	</sql>

	<select id="get" resultType="CcmLogTail">
		SELECT
		<include refid="ccmLogTailColumns" />
		FROM ccm_log_tail a
		<include refid="ccmLogTailJoins" />
		WHERE a.id = #{id}
	</select>

	<select id="findList" resultType="CcmLogTail">
		SELECT
		<include refid="ccmLogTailColumns" />
		FROM ccm_log_tail a
		<include refid="ccmLogTailJoins" />
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			<if test="relevanceId != null and relevanceId != ''">
				AND a.relevance_id = #{relevanceId}
			</if>
			<if test="relevanceTable != null and relevanceTable != ''">
				AND a.relevance_table = #{relevanceTable}
			</if>
			<if test="type != null and type != ''">
				AND a.type = #{type}
			</if>
			<if test="tailCase != null and tailCase != ''">
				AND a.tail_case LIKE
				<if test="dbName == 'oracle'">'%'||#{tailCase}||'%'</if>
				<if test="dbName == 'mssql'">'%'+#{tailCase}+'%'</if>
				<if test="dbName == 'mysql'">concat('%',#{tailCase},'%')</if>
			</if>
			<if test="tailPerson != null and tailPerson != ''">
				AND a.tail_person = #{tailPerson}
			</if>
			<if test="tailTime != null and tailTime != ''">
				AND a.tail_time = #{tailTime}
			</if>
			<if test="more1 != null and more1 != ''">
				AND a.more1 = #{more1}
			</if>
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
				ORDER BY a.update_date DESC
			</otherwise>
		</choose>
	</select>

	<select id="findAllList" resultType="CcmLogTail">
		SELECT
		<include refid="ccmLogTailColumns" />
		FROM ccm_log_tail a
		<include refid="ccmLogTailJoins" />
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
		</where>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
				ORDER BY a.update_date DESC
			</otherwise>
		</choose>
	</select>

	<insert id="insert">
		INSERT INTO ccm_log_tail(
		id,
		relevance_id,
		relevance_table,
		type,
		tail_case,
		tail_content,
		tail_person,
		tail_time,
		more1,
		more2,
		more3,
		create_by,
		create_date,
		update_by,
		update_date,
		remarks,
		del_flag,
		file
		) VALUES (
		#{id},
		#{relevanceId},
		#{relevanceTable},
		#{type},
		#{tailCase},
		#{tailContent},
		#{tailPerson},
		#{tailTime},
		#{more1},
		#{more2},
		#{more3},
		#{createBy.id},
		#{createDate},
		#{updateBy.id},
		#{updateDate},
		#{remarks},
		#{delFlag},
		#{file}
		)
	</insert>

	<update id="update">
		UPDATE ccm_log_tail SET
		relevance_id = #{relevanceId},
		relevance_table = #{relevanceTable},
		type = #{type},
		tail_case =
		#{tailCase},
		tail_content = #{tailContent},
		tail_person = #{tailPerson},
		tail_time = #{tailTime},
		more1 = #{more1},
		more2 = #{more2},
		more3 =
		#{more3},
		update_by = #{updateBy.id},
		update_date = #{updateDate},
		remarks = #{remarks},
		file = #{file}
		WHERE id = #{id}
	</update>

	<select id="findListByObject" resultType="CcmLogTail">
		SELECT
		<include refid="ccmLogTailColumns" />
		FROM ccm_log_tail a
		<include refid="ccmLogTailJoins" />
		<where>
			a.del_flag = #{DEL_FLAG_NORMAL}
			AND a.relevance_table =
			#{relevanceTable}
			AND a.relevance_id = #{relevanceId}
			<if test="tailCase !=null and tailCase !=''">
				AND a.tail_case = #{tailCase}
			</if>
		</where>
		ORDER BY a.update_date DESC
	</select>

	<update id="delete">
		UPDATE ccm_log_tail SET
		del_flag = #{DEL_FLAG_DELETE}
		WHERE id = #{id}
	</update>

	<insert id="insertLogTail" parameterType="java.util.List">
		INSERT INTO ccm_log_tail(
		id,
		relevance_id,
		relevance_table,
		type,
		tail_case,
		tail_content,
		tail_person,
		tail_time,
		more1,
		more2,
		more3,
		create_by,
		create_date,
		update_by,
		update_date,
		remarks,
		del_flag,
		file
		) VALUES 
		 <foreach collection="list" item="item" index="index" separator=",">
			(
		 	#{item.id},
			#{item.relevanceId},
			#{item.relevanceTable},
			#{item.type},
			#{item.tailCase},
			#{item.tailContent},
			#{item.tailPerson},
			#{item.tailTime},
			#{item.more1},
			#{item.more2},
			#{item.more3},
			#{item.createBy.id},
			#{item.createDate},
			#{item.updateBy.id},
			#{item.updateDate},
			#{item.remarks},
			#{item.delFlag},
			#{item.file}
			)
		</foreach>
	</insert>
</mapper>