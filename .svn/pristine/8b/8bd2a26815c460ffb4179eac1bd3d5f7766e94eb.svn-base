package com.arjjs.ccm.modules.ccm.sys.entity;

import com.arjjs.ccm.common.utils.excel.annotation.ExcelField;

/**
 * 日志统计导出实体类
 * @author cby
 *
 */
public class CcmWorkReportYO {
	
	private String name; 			//行政社区名称
	private String createName;  	// 创建人姓名
	private int jobnum;				//日常工作数量
	private int visitnum;			//日常走访数量
	private int eventnum;			//事件处理数量
	private int sum;				//总数
	
	@ExcelField(title="行政区划", align=2, sort=1)
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	@ExcelField(title="社工姓名", align=2, sort=2)
	public String getCreateName() {
		return createName;
	}

	public void setCreateName(String createName) {
		this.createName = createName;
	}
	
	@ExcelField(title="日常工作数量", align=2, sort=3)
	public int getJobnum() {
		return jobnum;
	}
	
	public void setJobnum(int jobnum) {
		this.jobnum = jobnum;
	}
	
	@ExcelField(title="日常走访数量", align=2, sort=4)
	public int getVisitnum() {
		return visitnum;
	}
	
	public void setVisitnum(int visitnum) {
		this.visitnum = visitnum;
	}
	
	@ExcelField(title="事件处理数量", align=2, sort=5)
	public int getEventnum() {
		return eventnum;
	}
	
	public void setEventnum(int eventnum) {
		this.eventnum = eventnum;
	}
	
	@ExcelField(title="总数", align=2, sort=6)
	public int getSum() {
		return sum;
	}
	
	public void setSum(int sum) {
		this.sum = sum;
	}
	
}
