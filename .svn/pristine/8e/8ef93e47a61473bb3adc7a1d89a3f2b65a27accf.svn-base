		a.event_scale AS "eventScale",
		a.event_type AS "eventType",
		a.number AS "number",
		a.property AS "property",
		a.case_condition AS
		"caseCondition",
		a.cul_papercode AS "culPapercode",
		a.cul_paperid AS
		"culPaperid",
		a.cul_name AS "culName",
		a.type_solve AS "typeSolve",
		a.num_crime AS "numCrime",
		a.num_flee AS "numFlee",
		a.num_arrest AS
		"numArrest",
		a.case_over_day AS "caseOverDay",
		a.case_solve AS
		"caseSolve",
		a.file1 AS "file1",
		a.file2 AS "file2",
		a.file3 AS "file3",
		a.status AS "status",
		a.create_by AS "createBy.id",
		a.create_date AS
