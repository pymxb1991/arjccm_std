		id,
		name,
		buildname,
		area_id,
		floor_area,
		piles_num,
		elem_num,
		build_num,
		build_peo,
		build_pname,
		sex,
		nation,
		content,
		birthday,
		education,
		tel,
		phone,
		residence,
		residencedetail,
		area_map,
		area_point,
		image,
		images,
		create_by,
		create_date,
		update_by,
		update_date,
		remarks,
		del_flag,
		build_code
		) VALUES (
		#{id},
		#{name},
		#{buildname},
		#{area.id},
		#{floorArea},
		#{pilesNum},
		#{elemNum},
		#{buildNum},
		#{buildPeo},
		#{buildPname},
		#{sex},
		#{nation},
		#{content},
		#{birthday},
		#{education},
		#{tel},
		#{phone},
		#{residence},
		#{residencedetail},
		#{areaMap},
		#{areaPoint},
		#{image},
		#{images},
		#{createBy.id},
		#{createDate},
		#{updateBy.id},
		#{updateDate},
		#{remarks},
		#{delFlag},
		#{code}
		)
	</insert>

	<update id="update">
		UPDATE ccm_house_buildmanage SET
		name = #{name},
		buildname = #{buildname},
		area_id = #{area.id},
		floor_area =
		#{floorArea},
		piles_num = #{pilesNum},
		elem_num = #{elemNum},
		build_num =
		#{buildNum},
		build_peo = #{buildPeo},
		build_pname = #{buildPname},
		sex =
		#{sex},
		nation = #{nation},
		content = #{content},
		birthday = #{birthday},
		education = #{education},
		tel = #{tel},
		phone = #{phone},
		residence =
		#{residence},
		residencedetail = #{residencedetail},
		area_map =
		#{areaMap},
		area_point = #{areaPoint},
		image = #{image},
		images = #{images},
