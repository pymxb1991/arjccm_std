<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
<title>公共厕所管理</title>
<meta name="decorator" content="default" />
<script type="text/javascript">
	$(document).ready(function() {

	});
	function page(n, s) {
		$("#pageNo").val(n);
		$("#pageSize").val(s);
		$("#searchForm").submit();
		return false;
	}
</script>
</head>
<body>
	<ul class="nav nav-tabs">
		<li class="active"><a href="${ctx}/live/ccmPlaceLive/">公共厕所列表</a></li>
		<%-- 		<shiro:hasPermission name="live:ccmPlaceLive:edit">
			<li><a href="${ctx}/live/ccmPlaceLive/form">公共厕所添加</a></li>
		</shiro:hasPermission> --%>
	</ul>
	<form:form id="searchForm" modelAttribute="ccmPlaceLive"
		action="${ctx}/live/ccmPlaceLive/2" method="post"
		class="breadcrumb form-search">
		<input id="pageNo" name="pageNo" type="hidden" value="${page.pageNo}" />
		<input id="pageSize" name="pageSize" type="hidden"
			value="${page.pageSize}" />
		<ul class="ul-form">
			<li><label>场所名称：</label> <form:input path="placeName"
					htmlEscape="false" maxlength="255" class="input-medium" /></li>
			<li><label>负责人姓名：</label> <form:input path="leaderName"
					htmlEscape="false" maxlength="255" class="input-medium" /></li>
			<li class="btns"><a
				onclick="parent.LayerDialog('${ctx}/live/ccmPlaceLive/form/toilet', '添加', '1200px', '900px')"
				class="btn btn-success">添加</a></li>
			<li class="btns"><input id="btnSubmit" class="btn btn-primary"
				type="submit" value="查询" /></li>
			<li class="clearfix"></li>
		</ul>
	</form:form>
	<sys:message content="${message}" />
	<table id="contentTable"
		class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>场所名称</th>
				<th>负责人姓名</th>
				<th>负责人联系电话</th>
				<th>关联组织机构</th>
				<th>地址</th>
				<th>属性</th>
				<th>是否收费</th>
				<th>入库时间</th>
				<shiro:hasPermission name="live:ccmPlaceLive:edit">
					<th>操作</th>
				</shiro:hasPermission>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${page.list}" var="ccmPlaceLive">
				<tr>
					<td><a class="btnList" onclick="parent.LayerDialog('${ctx}/live/ccmPlaceLive/form?id=${ccmPlaceLive.id}', '编辑', '1200px', '900px')" >
					${ccmPlaceLive.placeName}</a></td>
					<td>${ccmPlaceLive.leaderName}</td>
					<td>${ccmPlaceLive.leaderContact}</td>
					<td>${fns:getDictLabel(ccmPlaceLive.relevanceOrg, 'ccm_buss_cate', '无')}</td>
					<td>${ccmPlaceLive.address}</td>
					<td>${ccmPlaceLive.openProperty}</td>
					<td>${ccmPlaceLive.isFree}</td>
					<td><fmt:formatDate value="${ccmPlaceLive.createDate}"
							pattern="yyyy-MM-dd HH:mm:ss" /></td>
					<shiro:hasPermission name="live:ccmPlaceLive:edit">
						<td>
						<a class="btnList" onclick="parent.LayerDialog('${ctx}/live/ccmPlaceLive/form?id=${ccmPlaceLive.id}', '编辑', '1200px', '900px')" title="修改"><i class="icon-pencil"></i></a>
						<a class="btnList" href="${ctx}/live/ccmPlaceLive/delete?id=${ccmPlaceLive.id}" onclick="return confirmx('确认要删除该公共厕所吗？', this.href)" title="删除"><i class="icon-trash"></i></a>
							
							
							</td>
					</shiro:hasPermission>
				</tr>
			</c:forEach>
		</tbody>
	</table>
	<div class="pagination">${page}</div>
</body>
</html>