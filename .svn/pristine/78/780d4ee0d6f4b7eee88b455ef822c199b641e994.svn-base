* {
	margin: 0px;
	padding: 0px;
}

html, body {
	width: 100%;
	height: 100%;
	background-repeat: no-repeat;
	background-position: center;
	background-size: 100% 100%;
	overflow: hidden !important;
  background-color: #002e6c;
    color: #8ae7fa;
}
.navbar-inner{
	border:none;
}
#topImg{
	display: none;
}

.form-signin-heading {
	font-family: 微软雅黑;
	font-size: 25px;
	color: #ffffff;
	width: 100%;
	position: absolute;
	top: 5px;
}

label.error {
	background: none;
	width: 270px;
	font-weight: normal;
	color: inherit;
	margin: 0;
}

.top {
	position:relative;
	top: 0px;
	width: 100%;
	height:89px;
}

.form-top {
	color: #1b8ef4;
	font-weight: normal;
	position: relative;
	top: 20px;
	left: 33%;
}

.input-block-level {
	font-size: 18px !important;
	border: none !important;
	width: 280px;
	height: 50px !important;
	background-color: transparent !important;
	padding: 0px !important;
	margin-left: 35px;
	padding-left: 60px !important;
}

.username {
	background: url('../../../images/login_form_user.png') no-repeat
		center;
	margin-bottom: 20px !important;
	margin-top: 50px;
}

.password {
	background: url('../../../images/login_form_password.png') no-repeat
		center;
}

input::-webkit-input-placeholder {
	font-size: 14px;
}

.bodyBg {
	position: relative;
	width: 100%;
	height: 100%;
	background: url('../../../images/index-bg.png') center   no-repeat;
	 background-size: 100% 100%;
    position: relative;
}

