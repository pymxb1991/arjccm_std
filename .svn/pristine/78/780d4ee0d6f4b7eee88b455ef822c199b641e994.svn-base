}
.system-nav{
    width: calc(70% + 60px);
    margin: 0 auto;
    padding-top: 8%;
}
.system-nav li{
   width: 18%;
    height: 132px;
 
   /*  border-radius: 10px; */
    padding-top: 10px;
    font-size: 15px;
    float: left;
    margin-right: 1%;
    margin-left: 1%;
    margin-bottom: 15px;
/*    
   border: 1px solid #E9F3F4;
   background-color: rgba(200,200,200,0.1);
 */
  text-align: center;
 box-sizing: border-box;
   	background: url('../../../images/index-border1.png') center   no-repeat ;
	background-size: 100% 100%;

}
.system-nav li span{
 overflow: hidden;
    text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
    -webkit-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
	-ms-text-overflow: ellipsis;
     white-space: nowrap;
     display: block;
     height: 24px;
     line-height: 24px;
     margin-top: -15px;
    color: #fff;
     font-size: 20px;
     font-weight: 550;
     font-family: "Microsoft YaHei";
	background-image: -webkit-linear-gradient(left, rgba(138,231,250,0.1) 0%, rgba(138,231,250,0.2) 10%, rgba(138,231,250,0.3) 20%, rgba(138,231,250,0.5) 30%,   rgba(138,231,250,0.8) 40%, rgba(138,231,250,1) 50%, rgba(138,231,250,0.8) 60%, rgba(138,231,250,0.5) 70%, rgba(138,231,250,0.3) 80%, rgba(138,231,250,0.2) 90%, rgba(138,231,250,0.1));
	background-image: linear-gradient(to left, rgba(138,231,250,0.1) 0%, rgba(138,231,250,0.2) 10%, rgba(138,231,250,0.3) 20%, rgba(138,231,250,0.5) 30%,   rgba(138,231,250,0.8) 40%, rgba(138,231,250,1) 50%, rgba(138,231,250,0.8) 60%, rgba(138,231,250,0.5) 70%, rgba(138,231,250,0.3) 80%, rgba(138,231,250,0.2) 90%, rgba(138,231,250,0.1));
	-webkit-text-fill-color: transparent;

     -webkit-background-clip: text;          /*背景剪裁为文字，只将文字显示为背景*/

}
.system-nav .menu .nav-icon{
    font-size: 70px;
    /* color:#33aaff; */
    color:#8ae7fa;
}
.system-nav>li>a:hover {
	background: none;
	color: #eea807;
}
.system-nav li:hover  {
	/* border: 1px solid #20c694; */
}

.system-nav li:hover>a {
	color: #eea807;
	background: none;
}
#main {
    background: none !important;
}
