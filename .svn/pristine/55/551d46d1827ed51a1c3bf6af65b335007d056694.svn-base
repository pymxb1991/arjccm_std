                                    ${ccmEarlyWarning.rfid}
                            </td>
                        </c:when>
                        <c:when test="${controlBy==3}">
                            <td>
                                    ${ccmEarlyWarning.imei}
                            </td>
                        </c:when>
                        <c:when test="${controlBy==4}">
                            <td>
                                    ${ccmEarlyWarning.phone}
                            </td>
                        </c:when>
                        <c:otherwise>
                        </c:otherwise>
                    </c:choose>
                    <td>
                        <fmt:formatDate value="${ccmEarlyWarning.alarmDate}" pattern="yyyy-MM-dd HH:mm:ss"/>
