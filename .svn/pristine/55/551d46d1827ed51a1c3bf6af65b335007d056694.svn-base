                    <td>
                            <%--					<a href="${ctx}/device/ccmDeviceControl/form?id=${ccmDeviceControl.id}">--%>
                            ${ccmEarlyWarning.name}
                            <%--				</a>--%>
                    </td>
                    <td>
                            ${ccmEarlyWarning.sex}
                    </td>
                    <td>
                            ${ccmEarlyWarning.age}
                    </td>
                    <td>
                            ${ccmEarlyWarning.idCard}
                    </td>
                        <%--				<td>--%>
                        <%--					<fmt:formatDate value="${ccmDeviceControl.startDate}" pattern="yyyy-MM-dd HH:mm:ss"/>--%>
                        <%--				</td>--%>
