package com.arjjs.ccm.modules.plm.storage.entity;

/**
 * 该方法用于封装通过ajax请求的响应数据
 * @author Administrator
 *
 */
public class AjaxResultEntity {
	
	public static final int ERROR_PARAM = -7; //参数错误
	public static final int ERROR_OK = 0;  //成功
	
	private int ret = -1;   //响应状态 0：成功 -1：失败
	private Object result;   //响应结果数据
	private String message;  //提示信息
	
	public int getRet() {
		return ret;
	}
	public void setRet(int ret) {
		this.ret = ret;
	}
	public Object getResult() {
		return result;
	}
	public void setResult(Object result) {
		this.result = result;
	}
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}

}
