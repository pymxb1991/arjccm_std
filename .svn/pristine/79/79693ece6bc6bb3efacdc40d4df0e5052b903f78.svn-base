

<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>test</title>
  <script src="http://cdn.bootcss.com/jquery/1.12.3/jquery.min.js"></script>
  <script src="layer/layer.js"></script>
</head>
<body>


<script>

layer.ready(function(){
    var index=layer.open({
        type: 2,
        title: '事件消息',
        maxmin: true,
        shade: 0,
        closeBtn: 0,
        offset: 'rb',
        area: ['300px', '500px'],
        content: 'http://localhost:8080/arjccm/a/event/ccmEventIncident/list',
        //最小化按钮的回调
        min: function(layero){
            layero.css({left: 'auto', bottom: '0', right:'0',top:'auto'})
        },
        restore:function(layero){
            layero.css({left: 'auto', bottom: '0', right:'0',top:'auto'})
        }
    });
    layer.min(index)
});

</script>
</body>
</html>