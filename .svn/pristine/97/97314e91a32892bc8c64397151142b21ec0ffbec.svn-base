<%@ page contentType="text/html;charset=UTF-8" %>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
	<title>娱乐场所管理</title>
	<meta name="decorator" content="default"/>
	<script type="text/javascript">
		$(document).ready(function() {
			
		});
		function page(n,s){
			$("#pageNo").val(n);
			$("#pageSize").val(s);
			$("#searchForm").submit();
        	return false;
        }
	</script>
</head>
<body>
	<ul class="nav nav-tabs">
		<li class="active"><a href="${ctx}/casino/ccmPlaceCasino/01">公园和体育场列表</a></li>
	</ul>
	
	<form:form id="searchForm" modelAttribute="ccmPlaceCasino" 
		action="${ctx}/casino/ccmPlaceCasino/01" method="post" class="breadcrumb form-search">
		<input id="pageNo" name="pageNo" type="hidden" value="${page.pageNo}"/>
		<input id="pageSize" name="pageSize" type="hidden" value="${page.pageSize}"/>
		<ul class="ul-form">
			<li><label>场所名称：</label> <form:input path="placeName"
					htmlEscape="false" maxlength="255" class="input-medium" /></li>
			<li><label>负责人姓名：</label> <form:input path="leaderName"
					htmlEscape="false" maxlength="255" class="input-medium" /></li>
<%-- 			<li><label>创建时间：</label>
				<input name="beginCreateDate" type="text" readonly="readonly" maxlength="20" class="input-medium Wdate"
					value="<fmt:formatDate value="${ccmPlaceEducation.beginCreateDate}" pattern="yyyy-MM-dd HH:mm:ss"/>"
					onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',isShowClear:false});"/> - 
				<input name="endCreateDate" type="text" readonly="readonly" maxlength="20" class="input-medium Wdate"
					value="<fmt:formatDate value="${ccmPlaceEducation.endCreateDate}" pattern="yyyy-MM-dd HH:mm:ss"/>"
					onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',isShowClear:false});"/>
			</li> --%>
			<li class="btns">
				<a onclick="parent.LayerDialog('${ctx}/casino/ccmPlaceCasino/form?type=01', '添加', '1200px', '900px')"class="btn btn-success">
				添加
				</a>
			</li>
			<li class="btns"><input id="btnSubmit" class="btn btn-primary" type="submit" value="查询"/></li>
			
			<li class="clearfix"></li>
		</ul>
	</form:form>
	<sys:message content="${message}"/>
	<table id="contentTable" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>场所名称</th>
				<th>负责人姓名</th>
				<th>负责人联系电话</th>
				<th>关联组织机构</th>
				<th>更新时间</th>
				<shiro:hasPermission name="casino:ccmPlaceCasino:edit"><th>操作</th></shiro:hasPermission>
			</tr>
		</thead>
		<tbody>
		<c:forEach items="${page.list}" var="ccmPlaceCasino">
			<tr>
				<td><a class="btnList" onclick="parent.LayerDialog('${ctx}/casino/ccmPlaceCasino/form?id=${ccmPlaceCasino.id}', '编辑', '1200px', '900px')" >${ccmPlaceCasino.placeName}</a></td>
				<td>${ccmPlaceCasino.leaderName}</td>
				<td>${ccmPlaceCasino.leaderContact}</td>
				<td>${fns:getDictLabel(ccmPlaceCasino.relevanceOrg, 'ccm_buss_cate', '无')}</td>
				<td>
					<fmt:formatDate value="${ccmPlaceCasino.updateDate}" pattern="yyyy-MM-dd HH:mm:ss"/>
				</td>
				<shiro:hasPermission name="casino:ccmPlaceCasino:edit">
				<td>
					<a class="btnList" onclick="parent.LayerDialog('${ctx}/casino/ccmPlaceCasino/form?id=${ccmPlaceCasino.id}', '编辑', '1200px', '900px')" title="修改"><i class="icon-pencil"></i></a>
					<a class="btnList" href="${ctx}/casino/ccmPlaceCasino/delete?id=${ccmPlaceCasino.id}" onclick="return confirmx('确认要删除该娱乐场所吗？', this.href)" title="删除"><i class="icon-trash"></i></a>
				
				</td></shiro:hasPermission>
			</tr>
		</c:forEach>
		</tbody>
	</table>
	<div class="pagination">${page}</div>
</body>
</html>