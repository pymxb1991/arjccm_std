			LEFT JOIN sys_office office ON office.id = info.office_id
		WHERE
			info.alarm_time BETWEEN date_format( now( ), '%Y-%m-%d 00:00:00' )
			AND date_format( now( ), '%Y-%m-%d 23:59:59' )
			AND info.del_flag = 0
			AND info.office_id IS NOT NULL
		GROUP BY
