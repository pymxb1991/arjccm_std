.ol-popup {
	position: absolute;
	background-color: white;
	-webkit-filter: drop-shadow(0 1px 4px rgba(0, 0, 0, 0.2));
	filter: drop-shadow(0 1px 4px rgba(0, 0, 0, 0.2));
	padding: 8px;
	border-radius: 10px;
	/*border: 1px solid #fff;*/
	bottom: 12px;
	left: -50px;
	min-width: 300px;
	z-index: 199201;
	display: none;
}

.ol-popup table {
	width: 100%;
}

.ol-popup table td {
	white-space: nowrap;
/* 	border-bottom: 1px solid #e3e3e5; */
	padding: 3px;
	
}
/* .ol-popup.bb table td{
	color: #fff;
} */
td{
	font-size:14px;
}

.ol-popup table tr:last-child  td {
	border-bottom: 0
}
 .ol-popup:after, .ol-popup:before {
	top: 100%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
} 

.ol-popup:after {
	border-top-color: #fff;
	border-width: 10px;
	left: 48px;
	margin-left: -10px;
} 
.ol-popup:before {
	border-top-color: #cccccc;
	border-width: 11px;
	left: 48px;
	margin-left: -11px;
}

.ol-popup-closer {
	text-decoration: none;
	position: absolute;
	top: 2px;
	right: 8px;
}

