.common{
padding-right: 10px;
padding-left: 10px;
margin-top:12px;
}
.common .center{
	height:100%;
    width:100%;
    border:1px solid #eaeaea;
    border-radius:5px 5px 0 0;
}
.center-header{
	height: 45px;
	width:100%;
	line-height:45px;
	text-align:center;
	border-radius:5px 5px 0 0;
	color:#fff;
	font-size: 16px;
}
.center-header.center-echarts-name{
	color:#000;
	font-weight: bold;
	font-size: 15px;
	text-align: left;
}
.center-header.center-echarts-name span{
    display: block;
    width: 5px;
    height: 25px;
    margin-left: 15px;
    margin-right: 7px;
    float: left;
    margin-top: 11px;
}
.zongzhi-color1{
	background: #5dbe97
}
.zongzhi-color2{
	background: #66b9f2
}
.zongzhi-color3{
	background: #faba58
}
.zongzhi-color4{
	background: #ff988b
}
.zongzhi-color5{
	color:#7a7a79;
}
.center-main-header{
	text-align: center;
	font-size: 16px;
}
.center-main-center{
	text-align: center;
	margin-top: 10px;
}
.center-main-center i{
	margin-right: 5px;
}
.center-main-center span{
	color:#005ecc;
	margin-right: 5px;
	margin-left: 5px;
}