.header-logo{
	width: 1560px;
}
.header h5 {
    background: url(../images/headerLogo.png) no-repeat center;
    background-size: 100% 100%;
    line-height: 21px;
    letter-spacing: 10px;
    line-height: 40px;
    letter-spacing: 1px;
    font-size: 26px;
}
.header span {
    background: url(../images/header-right.png) no-repeat center;
    background-size: 100% 100%;
}
.header h5 b{
	font-size: 20px;
    font-weight: 545;
}
.fengxian-danwei{
	width: 100%;
	height: 100%;
	position: relative;
	
}
.fengxian-danwei li{
	width: 46%;
    height: 27%;
    float: left;
    margin: 2%;
	border: 1px solid #10559a;
	padding: 3px 5px
}
.fengxian-danwei li.maodn-li1{
	position: absolute;
	left:-3px;
	top:24px;
	width:50%;
	height:30%;
}
.maodn-li2{
	position: absolute;
	left: 128px;
    top: 31px;
	
}
.maodn-li3{
	position: absolute;
	left:0px;
	top:120px;

	
}
.maodn-li4{
	position: absolute;
	left: 128px;
    top: 120px;
	
}

.fengxian-color1{
	color:#800528;
	font-size: 18px;
	margin-bottom: 6px;
}
.fengxian-color2{
	color:#a69025
}
.fengxian-color3{
	color:#9fc325
}
.fengxian-color4{
	color:#dae5e8
}
.fengxian-num{
	color:#6f9fb3;
	font-size: 28px
}
.common-header {
    position: absolute;
    top: -5px;
    left: 32px;
     padding: 5px 16px;
}
.table-info-table{
	height: 100%; 
	width: 90%
}

