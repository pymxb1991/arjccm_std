}
	.fengxian-color1 {
    color: #800528;
    font-size: 15px;
    margin-bottom: 6px;
}
.maodn-li2 {
    left: 111px;
}
.maodn-li4 {
    left: 111px;
}
.table-info-table{
	height: 100%; 
