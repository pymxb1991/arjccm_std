    height:300px;
    border-radius:50%;
    font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    font-size:14px;
}
.inner-menu {
    position:relative;
	position: absolute;
    overflow: hidden;
	z-index: 32;
    width:300px;
    height:300px;
    border-radius:50%;
}
.toolmenu, .inner-menu ul {
    visibility:hidden;
    z-index:-99;
}
.inner-menu ul li {
    position: absolute;
    overflow: hidden;  
    transform-origin: 100% 100%; 
    left:0;
    top:0;
    width: 150px;
    height: 150px;
    background: rgba(118,195,249,.75); 
}

.toolmenu.active {
