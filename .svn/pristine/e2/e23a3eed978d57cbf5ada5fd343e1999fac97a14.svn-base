<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
<title>发起任务</title>
<meta name="decorator" content="default" />
<script type="text/javascript">
	$(document).ready(function() {
		top.$.jBox.tip.mess = null;
	});
	//详情弹框--刷新父页面
	function LayerDialog(src, title, height, width) {
		parent.layer.open({
			type : 2,
			title : title,
			area : [ height, width ],
			fixed : true, //固定
			maxmin : true,
			shade : false,
			//btn: ['确定', '关闭'], //可以无限个按钮
			content : src,
			end : function() {
				//location.reload();
			}
		});
	}
</script>
<style type="text/css">
.form-actions {
	margin-top: 0px;
	margin-bottom: 0px;
	border-color: white;
	background-color: white;
}

.left_div {
	width: 50%;
	float: left;
}

.right_div {
	width: 50%;
	float: left;
}

.table th{
	font-size: 15px;
	font-weight:bold;
}

h3{
    background-image: linear-gradient(to bottom,#fff,#e7eaed);
}

span{
	margin-left: 12px;
}

.table th, .table td {
	color: #333;
	background-color: white;
	background-image: none;
	border-top: 0px;
	vertical-align: middle /** 设置垂直方向居中 */
}

.table th {
	border-top: 1px solid rgb(221, 221, 221);
}
</style>
</head>
<body>
	<sys:message content="${message}" />
	<div class="left_div">
		<div class="form-actions">
			<h3><span>常用</span></h3>
			<table id="table" class="table   table-condensed"
				style="table-layout: fixed;">
				<tr>
					<th>流程名称</th>
					<th>发布时间</th>
					<th>操作</th>
				</tr>
				<c:forEach items="${page.list}" var="object">
					<c:set var="process" value="${object[0]}" />
					<c:set var="deployment" value="${object[1]}" />
					<tr>
						<c:if test="${process.category=='4' }">
							<td><a target="_blank"
								href="${ctx}/act/process/resource/read?procDefId=${process.id}&resType=image">${process.name}</a></td>
							<td><fmt:formatDate value="${deployment.deploymentTime}"
									pattern="yyyy-MM-dd HH:mm:ss" /></td>
							<td><a
								onclick="LayerDialog('${ctx}/act/task/form?procDefId=${process.id}', '新建流程', '80%', '80%')">新建流程</a>
							</td>
						</c:if>
					</tr>
				</c:forEach>
			</table>
		</div>
