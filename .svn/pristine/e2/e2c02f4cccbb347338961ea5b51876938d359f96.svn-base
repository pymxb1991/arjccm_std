.navbar-inner {
    background-color: #35404d;
    background-image: linear-gradient(to bottom,#35404d,#35404d);
    background-repeat: repeat-x;
    border: 1px solid #35404d;
}
#left {
    background: #2a3542;
}
#left .collapse {
    position: static;
    background: #35404d;
}
.accordion-inner{
     border-top: 1px solid #35404d;
}
.accordion-inner .nav-list>li>a{
    color: #999;
    text-shadow: none;
}
.accordion-group {
    margin-bottom: 2px;
     border: 0;
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
     border-radius: 0px;
    background: #2a3542;
}
.accordion-heading {
    white-space: nowrap;
    background-color: #fff;
    background-image: -moz-linear-gradient(top,#2a3542,#2a3542);
    background-image: -ms-linear-gradient(top,#2a3542,#2a3542);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#2a3542), to(#f5f5f5));
    background-image: -webkit-linear-gradient(top,#2a3542,#2a3542);
    background-image: -o-linear-gradient(top,#2a3542,#2a3542);
    background-image: linear-gradient(top,#2a3542,#2a3542);
    background-repeat: repeat-x;
}
.accordion-heading a{
    color: #aeb2b7;
}
.accordion-inner .nav-list>.active>a {
    color: #fff;
    text-shadow:none;
    background-color: #2fa4e7;
}
.table th{
    color: #5eb95e;
    background-color: rgba(94,185,94,.115);
    background-image: none;
}
