}

.alarm-value{
    margin-left: 10%;
}
 .alarm-name {
    display: inline-block;
    width: 60px;
    text-align: center;
}
 .alarm-name1 {
    background: #aa4644;
}
.alarm-name2 {
    background: #cc7b33;
}
.alarm-name3 {
