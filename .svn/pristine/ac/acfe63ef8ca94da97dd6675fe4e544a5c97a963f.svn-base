 p.notice {
    color: #fff;
    text-align: left;
    height: 55%;
    overflow: hidden;
     text-indent: 2em;
     display: -webkit-box;
     -webkit-line-clamp: 3;
     -webkit-box-orient: vertical;
}
#DutyUL .span6{
    padding-left: 5%;
}
#DutyUL  i {
    display: block;
    width: 10px;
    height: 10px;
    background-color: #1aa0ee;
    float: left;
    margin-top: 4px;
    margin-right: 10px;
}
#DutyUL  b {
   font-weight: normal;
}
.echartsAlarm-value  li {
    float: none;
    width: 114px;
    background-color: rgba(0,0,0,0.3);
