.alarm-rotate-div {
	  width: 68px;
    height:68px;
	position: relative;
	margin: auto;
}

.alarm-rotate-div .alarm-rotate-img {
	position: absolute;
	left: 0;
	top: 0;
}

.alarm-rotate-div .alarm-rotate-img1 {
	 width: 68px;
    height:68px;
	margin: 0px;
	border: 3px solid;
	border-left-color: #93a9d0;
	border-right-color: #93a9d0;
	border-top-color: transparent;
	border-bottom-color: transparent;
	border-radius: 50%;
	-webkit-animation: rotateAnimation1 4s infinite linear;
	-moz-animation: rotateAnimation1 4s infinite linear;
	-o-animation: rotateAnimation1 4s infinite linear;
	-os-animation: rotateAnimation1 4s infinite linear;
	animation: rotateAnimation1 4s infinite linear;
}

.alarm-rotate-img2 {
	 width:80px;
    height: 80px;
	margin: -6px;
	border: 3px solid #4573a7;
	border-radius: 50%;
	/* -webkit-animation: rotateAnimation 5s infinite  linear;
	-moz-animation: rotateAnimation 5s infinite  linear;
	-o-animation: rotateAnimation 5s infinite  linear;
	-os-animation: rotateAnimation 5s infinite  linear;
	animation: rotateAnimation 5s infinite  linear; */
}
@-webkit-keyframes rotateAnimation { 
0% {
	-ms-transform: rotate(0deg); /* IE 9 */
	-moz-transform: rotate(0deg); /* Firefox */
	-webkit-transform: rotate(0deg); /* Safari 鍜� Chrome */
	-o-transform: rotate(0deg); /* Opera */
	transform: rotate(0deg);
}

100%{
-ms-transform:rotate(-360deg); /* IE 9 */
-moz-transform:rotate(-360deg); /* Firefox */
-webkit-transform:rotate(-360deg); /* Safari 鍜� Chrome */
-o-transform:rotate(-360deg); /* Opera */
transform:rotate(-360deg);
}
}
@-webkit-keyframes rotateAnimation1 { 
	0% {
	-ms-transform: rotate(0deg); /* IE 9 */
	-moz-transform: rotate(0deg); /* Firefox */
	-webkit-transform: rotate(0deg); /* Safari 鍜� Chrome */
	-o-transform: rotate(0deg); /* Opera */
	transform: rotate(0deg);
}
100%{
-ms-transform:rotate(360deg); /* IE 9 */
-moz-transform:rotate(360deg); /* Firefox */
-webkit-transform:rotate(360deg); /* Safari 鍜� Chrome */
-o-transform:rotate(360deg); /* Opera */
transform:rotate(360deg);

}
