} */
.echartsAlarm-tab  li {
    width: 25%;
    float: left;
    line-height: 28px;
    text-align: center;
    cursor: pointer;
}
.echartsAlarm-tab > li.active, .echartsAlarm-tab > li:hover {
    background: url('../../tc2.png') no-repeat center;
    background-size: 100% 100%;
    background-color: rgba(45,84,159,0.3);
}

 #alarmTable {
     margin-top: 12px;
 }
.results{
    margin-top: 5%;
}
.results .span4{
    text-align: center;
}
.list_lh {
    height: 104%;
}
#AlarmUL {
    width: 100%;
    margin: 0 auto;
}
#AlarmUL li {
    padding: 1% 0;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    cursor: pointer;
}
.alarm1 b {
    color: #aa4644;
    font-weight: normal;
}
.alarm2 b {
    color: #cc7b33;
    font-weight: normal;
}
.alarm3 b{
    color: #89a54e;
    font-weight: normal;
}
 .alarm4 b{
     color: #18a73e;
     font-weight: normal;
 }

@media (max-width: 1366px) {
    p.notice {
        height: 47%;
    }
    #DutyUL .span6 {
         padding-left: 0%;
        min-height: 24px;
    }

    #echartsAlarm{
        height:140px
    }
    .echartsAlarm-value li {
        height: 20px;
        line-height: 20px;
        width: 91px;
    }
    .alarm-name {
        width: 50px;
    }
    #alarmTable td {
        line-height: 18px;
    }
    .echartsAlarm-tab li {
        line-height: 20px;
    }
    #alarmTable {
        margin-top: 5px;
    }
}
