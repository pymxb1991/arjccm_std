<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
<title>医疗卫生场所管理</title>
<meta name="decorator" content="default" />
<script type="text/javascript">
	$(document).ready(function() {

	});
	function page(n, s) {
		$("#pageNo").val(n);
		$("#pageSize").val(s);
		$("#searchForm").submit();
		return false;
	}
</script>
<script src="${ctxStatic}/ccm/event/js/ccmEventIncident.js"
	type="text/javascript"></script>
</head>
<body>
	<ul class="nav nav-tabs">
		<li class="active"><a href="${ctx}/hospital/ccmPlaceHospital/">私人诊所列表</a></li>
		<%-- <shiro:hasPermission name="hospital:ccmPlaceHospital:edit"><li><a href="${ctx}/hospital/ccmPlaceHospital/form">医疗卫生场所添加</a></li></shiro:hasPermission> --%>
	</ul>
	<form:form id="searchForm" modelAttribute="ccmPlaceHospital"
		action="${ctx}/hospital/ccmPlaceHospital/02" method="post"
		class="breadcrumb form-search">
		<input id="pageNo" name="pageNo" type="hidden" value="${page.pageNo}" />
		<input id="pageSize" name="pageSize" type="hidden"
			value="${page.pageSize}" />
		<ul class="ul-form">
			<li><label>场所名称：</label> <form:input path="placeName"
					htmlEscape="false" maxlength="255" class="input-medium" /></li>
			<li><label>负责人姓名：</label> <form:input path="leaderName"
					htmlEscape="false" maxlength="255" class="input-medium" /></li>
			<li class="btns"><a
				onclick="parent.LayerDialog('${ctx}/hospital/ccmPlaceHospital/form?type=02', '添加', '1100px', '700px')"
				class="btn btn-success"><i class="icon-plus"></i> 添加 </a></li>
			<li class="btns"><a href="javascript:;" id="btnSubmit"
				class="btn btn-primary"> <i class="icon-search"></i> 查询
			</a></li>
			<li class="clearfix"></li>
		</ul>
	</form:form>
	<sys:message content="${message}" />
	<table id="contentTable"
		class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>图片</th>
				<th>场所名称</th>
				<th>负责人姓名</th>
				<th>负责人联系电话</th>
				<th>医生数（人）</th>
				<th>护士数（人）</th>
				<th>入库时间</th>
				<shiro:hasPermission name="hospital:ccmPlaceHospital:edit">
					<th>操作</th>
				</shiro:hasPermission>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${page.list}" var="ccmPlaceHospital">
				<tr>
					<td width="200px"><img
						src="${ccmPlaceHospital.ccmBasePlace.placePicture}"
						style="height: 50px;" /></td>
					<td><a class="btnList"
						onclick="parent.LayerDialog('${ctx}/hospital/ccmPlaceHospital/form?id=${ccmPlaceHospital.id}', '编辑', '1100px', '700px')">
							${ccmPlaceHospital.placeName}</a></td>
					<td>${ccmPlaceHospital.leaderName}</td>
					<td>${ccmPlaceHospital.leaderContact}</td>
					<td>${ccmPlaceHospital.doctorNumber}</td>
					<td>${ccmPlaceHospital.nurseNumber}</td>
					<td><fmt:formatDate value="${ccmPlaceHospital.updateDate}"
							pattern="yyyy-MM-dd HH:mm:ss" /></td>
					<shiro:hasPermission name="hospital:ccmPlaceHospital:edit">
						<td><a class="btnList"
							onclick="parent.LayerDialog('${ctx}/hospital/ccmPlaceHospital/form?id=${ccmPlaceHospital.id}', '编辑', '1100px', '700px')"
							title="修改"><i class="icon-pencil"></i></a> <a class="btnList"
							href="${ctx}/hospital/ccmPlaceHospital/delete?id=${ccmPlaceHospital.id}"
							onclick="return confirmx('确认要删除该诊所吗？', this.href)" title="删除"><i
								class="icon-trash"></i></a></td>
					</shiro:hasPermission>
				</tr>
			</c:forEach>
		</tbody>
	</table>
	<div class="pagination">${page}</div>
</body>
</html>