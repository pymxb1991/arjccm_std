<%@ page contentType="text/html;charset=UTF-8" %>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
	<title>文化教育场所管理</title>
	<meta name="decorator" content="default"/>
	<script type="text/javascript">
		$(document).ready(function() {
			
		});
		function page(n,s){
			$("#pageNo").val(n);
			$("#pageSize").val(s);
			$("#searchForm").submit();
        	return false;
        }
	</script>
</head>
<body>
	<ul class="nav nav-tabs">
		<li class="active"><a href="${ctx}/education/ccmPlaceEducation/">文化教育场所列表</a></li>
		<shiro:hasPermission name="education:ccmPlaceEducation:edit">
		<li><a href="${ctx}/education/ccmPlaceEducation/form">文化教育场所添加</a></li></shiro:hasPermission>
	</ul>
	<form:form id="searchForm" modelAttribute="ccmPlaceEducation" action="${ctx}/education/ccmPlaceEducation/" method="post" class="breadcrumb form-search">
		<input id="pageNo" name="pageNo" type="hidden" value="${page.pageNo}"/>
		<input id="pageSize" name="pageSize" type="hidden" value="${page.pageSize}"/>
		<ul class="ul-form">
			<li><label>创建时间：</label>
				<input name="beginCreateDate" type="text" readonly="readonly" maxlength="20" class="input-medium Wdate"
					value="<fmt:formatDate value="${ccmPlaceEducation.beginCreateDate}" pattern="yyyy-MM-dd HH:mm:ss"/>"
					onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',isShowClear:false});"/> - 
				<input name="endCreateDate" type="text" readonly="readonly" maxlength="20" class="input-medium Wdate"
					value="<fmt:formatDate value="${ccmPlaceEducation.endCreateDate}" pattern="yyyy-MM-dd HH:mm:ss"/>"
					onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',isShowClear:false});"/>
			</li>
			<li class="btns"><input id="btnSubmit" class="btn btn-primary" type="submit" value="查询"/></li>
			<li class="clearfix"></li>
		</ul>
	</form:form>
	<sys:message content="${message}"/>
	<table id="contentTable" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>场所名称</th>
				<th>学校官网</th>
				<th>学校级别</th>
				<th>办学类型</th>
				<th>办公电话</th>
				<th>传真</th>
				<th>行政负责人姓名</th>
				<th>行政负责人联系方式</th>
				<th>修改时间</th>
				<shiro:hasPermission name="education:ccmPlaceEducation:edit">
				<th>操作</th></shiro:hasPermission>
			</tr>
		</thead>
		<tbody>
		<c:forEach items="${page.list}" var="ccmPlaceEducation">
			<tr>
				<td>${ccmPlaceEducation.placeName}</td>
				<td><a href= "${ccmPlaceEducation.schoolNet}">${ccmPlaceEducation.schoolNet}</a></td>
				<td>${ccmPlaceEducation.schoolRank}</td>
				<td>${ccmPlaceEducation.schoolType}</td>
				<td>${ccmPlaceEducation.schoolPhone}</td>
				<td>${ccmPlaceEducation.faxNumber}</td>
				<td>${ccmPlaceEducation.managerName}</td>
				<td>${ccmPlaceEducation.managerPhoneNumber}</td>
				<td>
					<a href="${ctx}/education/ccmPlaceEducation/form?id=${ccmPlaceEducation.id}">
					<fmt:formatDate value="${ccmPlaceEducation.updateDate}" pattern="yyyy-MM-dd HH:mm:ss"/>
					</a>
				</td>
				<shiro:hasPermission name="education:ccmPlaceEducation:edit">
				<td>
				    <a onclick="parent.LayerDialog('${ctx}/education/ccmPlaceEducation/form?id=${ccmPlaceEducation.id}', '编辑', '1200px', '900px')">编辑</a>
    				<%-- <a href="${ctx}/education/ccmPlaceEducation/form?id=${ccmPlaceEducation.id}">修改</a> --%>
					<a href="${ctx}/education/ccmPlaceEducation/delete?id=${ccmPlaceEducation.id}" onclick="return confirmx('确认要删除该文化教育场所吗？', this.href)">删除</a>
				</td></shiro:hasPermission>
			</tr>
		</c:forEach>
		</tbody>
	</table>
	<div class="pagination">${page}</div>
</body>
</html>