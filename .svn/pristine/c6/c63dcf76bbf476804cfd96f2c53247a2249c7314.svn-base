.common{
padding-right: 10px;
padding-left: 10px;
margin-top:12px;
}
.common .center{
	height:90%;
    width:92%;
	margin-left: 23px;
	margin-top: 11px;
    border:1px solid #EBECED;
    box-shadow:3px 2px 10px #EBECED;
    border-radius:5px 5px 0 0;
}
.center-header{
	height: 45px;
	width:100%;
	line-height:45px;
	text-align:left;
	/*border-radius:5px 5px 0 0;*/
	color:#fff;
	font-size: 16px;
    padding-left: 5%;
}
.center-header.center-echarts-name{
	color:#000;
	font-weight: bold;
	font-size: 15px;
	text-align: left;
}
.center-header.center-echarts-name span{
    display: block;
    width: 5px;
    height: 25px;
    margin-left: 15px;
    margin-right: 7px;
    float: left;
    margin-top: 11px;
}
.center-header a{
    color: #fff;
    font-weight: bold;
    font-size: 16px;
	font-family:Microsoft YaHei;
}
.danghang{
	color: #787878;
	font-family:Microsoft YaHei;
	font-size:12px;
	font-weight:400;
	line-height:12px;
	top: 22px;
	position: relative;
	margin-left: 3px;
}
.zongzhi-color1{
	background: url('../images/indexZongZhi/dataImg01.png') no-repeat center;
	background-size: 100% 100%;
}
.zongzhi-color2{
	background: url('../images/indexZongZhi/dataImg02.png') no-repeat center;
	background-size: 100% 100%;
}
.zongzhi-color3{
	background: url('../images/indexZongZhi/dataImg03.png') no-repeat center;
	background-size: 100% 100%;
}
.zongzhi-color4{
	background: url('../images/indexZongZhi/dataImg04.png') no-repeat center;
	background-size: 100% 100%;
}
.zongzhi-color5{
	color:#7a7a79;
}
.center-main-header{
	text-align: center;
	font-size: 16px;
}
.center-main-center{
	text-align: center;
	margin-top: 10px;
}
.center-main-center i{
	margin-right: 5px;
}
.center-main-center span{
	color:#005ecc;
	margin-right: 5px;
	margin-left: 5px;
}
.hrLine {
	height: 1px;
    clear: both;
    background-image: linear-gradient(to right, rgba(215,225,237,0), rgba(215,225,237,1), rgba(215,225,237,1), rgba(215,225,237,1), rgba(215,225,237,0));
}
.hrMargin{
	margin-top: 20px;
	clear: both;
	height: 16px;
}
.labelDiv{
	/* color:#333333; *//*此颜色只是适应于天蓝主题，暂时注释掉*/
	font-size:12px;
	font-family:Microsoft YaHei;
	font-weight:400;
	line-height:12px;
	text-align: left;
}
.textDiv{
	font-size:16px;
	font-family:Microsoft YaHei;
	font-weight:bold;
	/* color:#162139; *//*此颜色只是适应于天蓝主题，暂时注释掉*/
	line-height:12px;
	text-align: right;
	padding: 0;
}
.dataDiv{
	margin-top: 20px;
}
.echartsLabel{
	display: inline;
	font-size:14px;
	font-family:Microsoft YaHei;
	font-weight:bold;
	/* color:#555559; *//*此颜色只是适应于天蓝主题，暂时注释掉*/
}
#PeopleStatistics{
	float: right;
	border-color: transparent;
	background-color: transparent;
}
#PeopleStatistics a{
	background-color: transparent;
}
#peopleKym{
	float: right;
	border-color: transparent;
	background-color: transparent;
}
#peopleKym a{
	background-color: transparent;
	border-color: transparent;
}
.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover{
	border: 0;
    border-bottom: 1px solid #2B8BEF;
}
.nav>li>a:focus, .nav>li>a:hover{
    background-color: transparent;
    border: 0;
    border-bottom: 1px solid #2B8BEF;
}
.nav>li>a{
	font-size:12px;
	font-family:Microsoft YaHei;
	font-weight:400;
	color:#555559;
}
.nav>li.active>a{
	font-size:12px;
	font-family:Microsoft YaHei;
	font-weight:bold;
	color:#555559;
}